package intro;
import java.util.Scanner;

public class ttable {

	public static void main (String[] args) {
		PrintTable(3, 5);
	}
	
	private static void PrintTable (int x, int y) {
		//Execute this code y amount of times
		for(int i = 0; i < y; i++)
		{
			//Print number * 1(+1) = sum
			System.out.println(x + "*" + (i + 1) + " = " + (x + (i+1)));
		}
	}
}
