package intro;

public class sample {
//Complete beginner to Java, this is mainly for playing around/note taking

	public static void main(String[] args) {
		
	}
	
	private static void HelloWorld () {
		//This is how you print code to the console.
		System.out.println("Hello World");
	}
	
	private static void CalcSum (int val1, int val2) {
		int sum;
		sum = val1 + val2;
		System.out.println("The sum is: " + sum);
	}
	
	private static void CalcProduct (int val1, int val2) {
		int product;
		product = val1 * val2;
		System.out.println("The product is: " + product);
	}
}
